{
  "name": "jheadx",
  "version": "0.0.1",
  "description": "`jhead` wrapper with extended functionality to batch fix photos lacking EXIF data",
  "licence": "GPL-3.0",
  "main": "index.js",
  "repository": "git@github.com:adrw/jheadx.git",
  "author": "adrw Andrew Alexander <account+npmjs@adrw.ch>",
  "bin": {
    "jheadx": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">= 0.2.0",
    "yarn": "YARN NO LONGER USED - use npm instead."
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "commander": "^2.18.0",
    "dayjs": "^1.7.7",
    "inquirer": "^6.2.0",
    "node-emoji": "^1.8.1",
    "yargs": "^12.0.4"
  },
  "devDependencies": {
    "@misk/tslint": "^0.1.3-11",
    "@types/inquirer": "0.0.43",
    "@types/node": "^10.12.18",
    "@types/node-emoji": "^1.8.0",
    "@types/yargs": "^12.0.1",
    "jest": "^23.6.0",
    "tslib": "^1.9.3",
    "typescript": "^3.2.2"
  },
  "preferGlobal": true,
  "scripts": {
    "start": "node dist/index.js",
    "watch": "tsc -w",
    "build": "tsc",
    "test": "jest",
    "lint": "prettier --write --config package.json \".{/src/**/,/}*.{md,css,sass,less,json,js,jsx,ts,tsx}\"",
    "prepublish": "npm run-script build",
    "smooth-release": "smooth-release",
    "precommit": "npm run-script lint"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 80,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
